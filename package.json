{
  "name": "waveline-server",
  "version": "1.0.0",
  "description": "Waveline Music Server",
  "main": "dist/app.js",
  "bin": "dist/app.js",
  "scripts": {
    "test": "ava test/**/*.js",
    "pretest": "tsc --experimentalDecorators test/test.ts",
    "lint": "tslint 'src/**/*.ts'",
    "prebuild": "yarn clean",
    "clean": "rimraf dist && rimraf test/**/*.js",
    "build": "tsc",
    "dev": "nodemon -e ts  --exec \"yarn start\"",
    "start": "yarn lint && tsc --project tsconfig.json && node dist/app.js"
  },
  "devDependencies": {
    "@types/cors": "^2.8.0",
    "@types/dotenv": "^4.0.0",
    "@types/formidable": "^1.0.29",
    "@types/ip": "^0.0.30",
    "@types/mime": "^2.0.0",
    "@types/mongoose": "^5.2.0",
    "@types/node": "^11.9.5",
    "ava": "^1.2.1",
    "nodemon": "^1.11.0",
    "rimraf": "^2.5.4",
    "supertest": "^3.0.0",
    "tslint": "^4.3.1",
    "typescript": "^3.3.3333"
  },
  "pkg": {
    "scripts": "dist/**/*.js"
  },
  "author": "Mihkel Baranov",
  "license": "MIT",
  "dependencies": {
    "@wellenline/via": "https://github.com/wellenline/via#rev2",
    "album-art": "^2.0.1",
    "axios": "^0.18.1",
    "cors": "^2.8.1",
    "dotenv": "^4.0.0",
    "formidable": "^1.1.1",
    "ip": "^1.1.5",
    "laf-http": "^3.0.0",
    "mime": "^2.2.0",
    "mongoose": "^5.2.3",
    "music-metadata": "^2.6.1",
    "qrcode-terminal": "^0.12.0",
    "typegoose": "^5.5.1"
  },
  "repository": "https://github.com/MihkelBaranov/waveline-server.git"
}
